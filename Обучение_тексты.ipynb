    "                'sgdc__eta0':[0.01, 0.05, 0.1, 0.2, 0.3, 0.5]}]\n",
    "\n",
    "\n",
    "random_search_sgdc = RandomizedSearchCV(pipeline_sgdc, \n",
    "                                        params_sgdc, \n",
    "                                        scoring='f1',\n",
    "                                        cv=StratifiedKFold(n_splits=3).split(X_train, y_train),\n",
    "                                        n_jobs=-1\n",
    "                                        )\n",
    "random_search_sgdc.fit(X_train['lemm_text'], y_train)"
